{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "feature1-df"
		},
		"ADLS_API_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_API'"
		},
		"feature1_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'feature1_databricks'"
		},
		"ADLS_API_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlstestenvironment1.dfs.core.windows.net/"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/"
		},
		"RestAPI_dataset_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/"
		},
		"feature1_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0218-050458-2tgsc7fm"
		},
		"feature1_source_http_linkservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "feature1-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_API_accountKey": {
						"value": "[parameters('ADLS_API_accountKey')]"
					},
					"feature1_databricks_accessToken": {
						"value": "[parameters('feature1_databricks_accessToken')]"
					},
					"ADLS_API_properties_typeProperties_url": {
						"value": "[parameters('ADLS_API_properties_typeProperties_url')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"RestAPI_dataset_properties_typeProperties_url": {
						"value": "[parameters('RestAPI_dataset_properties_typeProperties_url')]"
					},
					"feature1_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('feature1_databricks_properties_typeProperties_existingClusterId')]"
					},
					"feature1_source_http_linkservice_properties_typeProperties_url": {
						"value": "[parameters('feature1_source_http_linkservice_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}